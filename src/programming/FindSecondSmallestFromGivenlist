import java.util.*;
class Main {
    public static void main(String[] args) {
    
      List<Integer> li = Arrays.asList(20, 67, 34, 37);
     int smallest=Integer.MAX_VALUE;
      int secondsmall=Integer.MAX_VALUE;
      for(int num:li)
      {
          if(num<smallest)
          {
              secondsmall=smallest;
              smallest=num;
          }
          else if(num<secondsmall && num!=smallest)
          {
              secondsmall=num;
          }
      }

        System.out.println("Smallest: " + smallest);//20
        System.out.println("Second Smallest: " + secondsmall);//34
    }
}
