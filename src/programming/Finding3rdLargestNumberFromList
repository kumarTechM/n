// Online Java Compiler
// Use this editor to write, compile and run your Java code online
import java.util.*;
import java.util.Map.Entry;
class Main {
    
  
    public static void main(String[] args) 
    {
        int[] arr = {10, 5, 20, 8, 15, 20, 25, 5};
     Arrays.sort(arr);
     System.out.println("sorted array is"+Arrays.toString(arr));
     System.out.println(arr[arr.length-3]); // with duplicate 20
    
    
   // Use TreeSet to remove duplicates and sort
        TreeSet<Integer> set = new TreeSet<>();
        for(int a:arr)
        {
            set.add(a);
        }
        System.out.println(set);
        for (int num : arr) set.add(num);
      List<Integer> list = new ArrayList<>(set);
        int thirdLargest = list.get(list.size()-3);
       int thirdSmallest=list.get(2);

   //sorted list  is :[5, 8, 10, 15, 20, 25]--we removed duplicaed and apply sorting using treset
        System.out.println("3rd Largest without duplicates: " + thirdLargest);//15
        System.out.println("3rd smallest  without duplicates: " + thirdSmallest);//10 
    }
}

normal way 
// Online Java Compiler
// Use this editor to write, compile and run your Java code online
import java.util.*;
class Main {
    public static void main(String[] args) {
    
      List<Integer> li = Arrays.asList(20, 67, 34, 37,90,8);
      int Largest=Integer.MIN_VALUE;
      int SecondLargest=Integer.MIN_VALUE;
           int thirdLargest=Integer.MIN_VALUE;
      int smallest=Integer.MAX_VALUE;
      int seocndSmallest=Integer.MAX_VALUE;
      for(int num:li)
      {
          if(num>Largest)
          {
             thirdLargest = SecondLargest;
                SecondLargest = Largest;
                Largest = num;
          }
          else if(num>SecondLargest && num!=Largest)
          {
              SecondLargest=num;
          }
          else if(num>thirdLargest&& num!=SecondLargest)
          {
              thirdLargest=num;
          }
          if(num<smallest)
          {
              seocndSmallest=smallest;
              smallest=num;
          }
          else if(num<seocndSmallest&& num!=smallest)
          {
              seocndSmallest=num;
          }
      }
      System.out.println(Largest);
       System.out.println(SecondLargest);
        System.out.println("third lare"+thirdLargest);
        System.out.println("smallest"+smallest);
       System.out.println("seocndSmallest"+seocndSmallest);
      
    }
}
